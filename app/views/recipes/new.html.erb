    <%= form_for @recipe do |temp| %>
        <%= temp.label :title %>
        <%= temp.text_field :title %>

        <br>

        <%= temp.fields_for :ingredients do |ing| %>

            <%= ing.label :name %>
            <%= ing.text_field :name %>

            <%= ing.label :quanitity %>
            <%= ing.text_field :quantity %>

            <br>

        <% end %>

        <%= temp.submit %>
    <% end %>
